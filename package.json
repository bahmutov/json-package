{
  "name": "json-package",
  "description": "Fetches any property from package.json using short prefix `jso dep` is same as `cat package.json | grep dependencies`",
  "main": "src/json-package.js",
  "bin": {
    "j": "bin/jso.js",
    "jso": "bin/jso.js"
  },
  "preferGlobal": true,
  "version": "0.0.0-semantic-release",
  "scripts": {
    "test": "node --harmony node_modules/.bin/rocha src/*-spec.js",
    "lint": "standard bin/*.js src/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-issues && commit-wizard",
    "issues": "git-issues",
    "all": "node --harmony bin/jso.js",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/json-package.git"
  },
  "files": [
    "bin",
    "src/*.js",
    "!src/*-spec.js"
  ],
  "keywords": [
    "json",
    "package",
    "show",
    "cli",
    "utility",
    "short",
    "prefix",
    "npm"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/json-package/issues"
  },
  "homepage": "https://github.com/bahmutov/json-package#readme",
  "devDependencies": {
    "check-more-types": "2.3.0",
    "git-issues": "1.2.0",
    "lazy-ass": "1.3.0",
    "mocha": "2.3.4",
    "pre-git": "2.0.2",
    "really-need": "1.7.2",
    "rocha": "1.6.1",
    "semantic-release": "^4.3.5",
    "standard": "5.4.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm run lint",
        "npm test",
        "node --harmony bin/jso.js"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "dependencies": {
    "findup": "0.1.5",
    "simple-bin-help": "1.4.0"
  }
}
